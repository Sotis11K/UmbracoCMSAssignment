@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
    var pageTitle = !string.IsNullOrEmpty(ViewBag.title) ? $"{ViewBag.Title} | Onatrix - Financial Solutions for Your Buisness" : "Onatrix - Financial Solution for Your Buisness";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="https://kit.fontawesome.com/a56612222" crossorigin="annonymous" />
    <link rel="stylesheet" href="/css/main.css" />
    <link rel="stylesheet" href="/css/about.css" />
    <link rel="stylesheet" href="/css/services.css" />
    <link rel="stylesheet" href="/css/ourProjects.css" />
    <link rel="stylesheet" href="/css/riskManagement.css" />
    <link rel="stylesheet" href="/css/contact.css" />
    <link rel="stylesheet" href="/css/annualreport.css" />




    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <title>@pageTitle</title>
</head>

<body>

    <div class="wrapper">
        <header>
            @await Html.PartialAsync("headerSectionPartial")
        </header>


        <main>
			@RenderBody()
        </main>


        <footer>
            @await Html.PartialAsync("footerSectionPartial")
        </footer>
    </div>

    <script src="/scripts/main.js"></script>
    <script src="https://kit.fontawesome.com/f53f99e76c.js" crossorigin="anonymous"></script>

</body>
</html>
