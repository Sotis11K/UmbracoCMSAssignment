@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = "mainLayout.cshtml";

	var partner1 = Model?.Value<IPublishedContent>("sponsor1")?.Url() ?? "";
	var partner2 = Model?.Value<IPublishedContent>("sponsor2")?.Url() ?? "";
	var partner3 = Model?.Value<IPublishedContent>("sponsor3")?.Url() ?? "";
	var partner4 = Model?.Value<IPublishedContent>("sponsor4")?.Url() ?? "";
	var partner5 = Model?.Value<IPublishedContent>("sponsor5")?.Url() ?? "";
	var partner6 = Model?.Value<IPublishedContent>("sponsor6")?.Url() ?? "";

}

<section id="section-contact">
	<div class="content">
		<h1>@Model?.Value("contactUsHeadline")</h1>
		<div class="navigation-container">
			<a href="/">Home</a>
			<p id="arrow">></p>
			<p>@Model?.Value("contactUsNavigation")</p>
		</div>
	</div>
</section>

@await Html.PartialAsync("letUsKnowAboutYourProject")

<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2035.1615241962002!2d18.051314413085485!3d59.330257074508864!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x465f9d61b5b2a595%3A0xd9fa560691e12d7!2sKlarabergsviadukten%2090%2C%20111%2064%20Stockholm!5e0!3m2!1ssv!2sse!4v1728155767920!5m2!1ssv!2sse" width="100%" height="700" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>

<section id="section-your-project-form">
	<div class="container">
		<h5>@Model?.Value("callBackFormTitle")</h5>
		<h2>@Model?.Value("callBackFormHeadline")</h2>
		<p>@Model?.Value("callBackFormText")</p>
		<form>
			<div class="input-container">
				<input placeholder="Name"/>
				<input placeholder="Phone" />
				<input placeholder="Email address" />
				<select>
					<option>Financial consulting</option>
					<option>Option 2</option>
					<option>Option 3</option>
					<option>Option 4</option>
					<option>Option 5</option>
				</select>
			</div>
			<button type="submit">Submit</button>
		</form>
	</div>
</section>

<section id="section-sponsors">
	<div class="container">
		<div class="content">
			<img src="@partner1" />
			<img src="@partner2" />
			<img src="@partner3" />
			<img src="@partner4" />
			<img src="@partner5" />
			<img src="@partner6" />
		</div>
	</div>
</section>